[display]

window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="ycfg_alpha"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/csgtomesh/plugin.cfg")

